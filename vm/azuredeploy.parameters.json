{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "sharedImageId": {
      "value": "/subscriptions/111111111111111/resourceGroups/<resource-group>/providers/Microsoft.DevTestLab/labs/<devtest-lab>/sharedgalleries/<shared-gallery>/sharedimages/<image-name>"
    },          
    "labName": {
      "value": "<lab-name>"
    },
    "newVMName": {
      "value": "<vm-name>"
    },
    "userName": {
      "value": "<username>"
    },
    "password": {
      "value": "<password>"
    },
    "mongoDbConnectionString": {
      "reference": {
        "keyVault": {
          "id": "/subscriptions/111111111/resourceGroups/<resource-group>/providers/Microsoft.KeyVault/vaults/dsf-kv"
        },       
        "secretName": "mongo-db-conn-string"
      }
    }
  }
}
