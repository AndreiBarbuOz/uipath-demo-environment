{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "input1": {
      "type": "string"
    },
    "input2": {
      "type": "string"
    }
  },
  "variables": {
  },
  "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2019-10-01",
      "name": "presales",
      "dependsOn": [
      ],
      "properties": {
        "mode": "Incremental",
        "expressionEvaluationOptions": {
          "scope": "inner"
        },
        "parameters": {
          "input1": {
            "value": "[parameters('input1')]"
          },
          "input2": {
            "value": "[parameters('input2')]"
          },
          "conn_string": {
            "reference": {
              "keyVault": {
                "id": "[resourceId(resourceGroup().tags.Rg, 'Microsoft.KeyVault/vaults', concat(resourceGroup().tags.ProjectName, '-kv'))]"
              },
              "secretName": "mongo-db-conn-string"
            }
          }
        },
        "template": {
          "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
          "contentVersion": "1.0.0.0",
          "parameters": {
            "input1": {
              "type": "string"
            },
            "input2": {
              "type": "string"
            },
            "conn_string": {
              "type": "string"
            }
          },
          "variables": {},
          "resources": [],
          "outputs": {
            "resourceGroupOutput": {
              "value": "[resourceGroup()]",
              "type": "object"
            },
            "conn_string": {
              "value": "[parameters('conn_string')]",
              "type": "object"
            }
          }
        }
      }
    }
  ],
  "outputs": {
    "outerRG": {
      "value": "[resourceGroup()]",
      "type": "object"
    },
    "innerRG": {
      "value": "[reference('presales').outputs.resourceGroupOutput]",
      "type": "object"
    }
  }
}